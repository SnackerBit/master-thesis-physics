@article{cite:area_law_1D_proof,
	doi = {10.1088/1742-5468/2007/08/P08024},
	url = {https://dx.doi.org/10.1088/1742-5468/2007/08/P08024},
	year = {2007},
	month = {aug},
	publisher = {},
	volume = {2007},
	number = {08},
	pages = {P08024},
	author = {M B Hastings},
	title = {An area law for one-dimensional quantum systems},
	journal = {Journal of Statistical Mechanics: Theory and Experiment},
	abstract = {We prove an area law for the entanglement entropy in gapped one-dimensional quantum systems. The bound on the entropy grows surprisingly rapidly with the correlation length; we discuss this in terms of properties of quantum expanders and present a conjecture on matrix product states which may provide an alternate way of arriving at an area law. We also show that, for gapped, local systems, the bound on Von Neumann entropy implies a bound on Rényi entropy for sufficiently large α&lt;1  and implies the ability to approximate the ground state by a matrix product state.}
}

